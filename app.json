{
  "expo": {
    "name": "kumi",
    "slug": "kumi-attendance",
    "scheme": "kumi",
    "version": "1.0.0",
    "icon": "./assets/kumi-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/kumi-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f9ff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kumonpunggolplaza.kumi"
    },
    "android": {
      "package": "com.kumonpunggolplaza.kumi",
      "adaptiveIcon": {
        "foregroundImage": "./assets/kumi-logo.png",
        "backgroundColor": "#f0f9ff"
      },
      "permissions": ["android.permission.CAMERA"]
    },
    "web": {
      "favicon": "./assets/kumi-logo.png"
    },
    "experiments": {
      "reactCanary": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-av",
      "expo-build-properties"
    ],
    "extra": {
      "EXPO_PUBLIC_API_BASE": "http://46.62.157.49/",
      "eas": {
        "projectId": "ac98a74c-03e4-4dee-9e04-acbdf735e2a7"
      }
    }
  }
}
